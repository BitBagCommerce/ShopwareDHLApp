{
    "name": "bitbag/shopware-app-skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A minimal Shopware app project recommended to create new apps",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-soap": "*",
        "alexcherniatin/dhl24-webapi-php-sdk": "^1.0",
        "bitbag/shopware-app-system-bundle": "dev-bugfix/missing-event-data",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "guzzlehttp/guzzle": "^7.4",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/asset": "5.4",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/monolog-bundle": "^3.7",
        "symfony/translation": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/yaml": "^5.4",
        "twig/extra-bundle": "^3.3",
        "twig/twig": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.0"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "BitBag\\ShopwareAppSkeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BitBag\\ShopwareAppSkeleton\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "repositories": [
        {
            "type":"vcs",
            "url": "https://github.com/BitBagCommerce/BitBagShopwareAppSystemBundle"
        }
    ],
    "scripts": {
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.4"
        }
    },
    "require-dev": {
        "bitbag/coding-standard": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.5",
        "pact-foundation/pact-php": "^7.1",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-symfony": "^3.1",
        "symfony/maker-bundle": "^1.38",
        "vimeo/psalm": "^4.19"
    },
    "minimum-stability": "dev"
}
