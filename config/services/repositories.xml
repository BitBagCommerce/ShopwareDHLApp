<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service class="BitBag\ShopwareAppSkeleton\Repository\ShopRepository"
                 id="bitbag.shopware_app_skeleton.repository.shop_repository">
            <argument type="service" id="doctrine"/>
        </service>

        <service class="BitBag\ShopwareAppSkeleton\Repository\ConfigRepository"
                 id="bitbag.shopware_dhl_app.repository.config_repository">
            <argument type="service" id="doctrine"/>
        </service>

        <service class="BitBag\ShopwareAppSkeleton\Repository\LabelRepository"
                 id="bitbag.shopware_dhl_app.repository.label_repository">
            <argument type="service" id="doctrine"/>
        </service>
    </services>
</container>
