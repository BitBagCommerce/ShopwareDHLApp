<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service class="BitBag\ShopwareDHLApp\API\Shopware\CustomFieldSetCreator"
                 id="bitbag.shopware_dhl_app.api.create_custom_field_set">
            <argument type="service" id="bitbag.shopware_dhl_app.api.custom_field_api_service"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.create_custom_field_set"/>
            <argument type="service" id="shopware.repository.custom_field_set"/>

        </service>

        <service class="BitBag\ShopwareDHLApp\API\Shopware\CustomFieldsCreator"
                 id="bitbag.shopware_dhl_app.api.create_custom_fields">
            <argument type="service" id="bitbag.shopware_dhl_app.factory.custom_field_filter"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.create_custom_field"/>
            <argument type="service" id="bitbag.shopware_dhl_app.api.create_custom_field_set"/>
            <argument type="service" id="shopware.repository.custom_field"/>
        </service>

        <service class="BitBag\ShopwareDHLApp\API\DHL\ApiResolver"
                 id="bitbag.shopware_dhl_app.api.api_resolver">
            <argument type="service" id="bitbag.shopware_dhl_app.repository.config_repository"/>
        </service>

        <service class="BitBag\ShopwareDHLApp\API\Shopware\AvailabilityRuleCreator"
                 id="bitbag.shopware_dhl_app.api.availability_rule_creator">
            <argument type="service" id="shopware.repository.rule"/>
        </service>

        <service class="BitBag\ShopwareDHLApp\API\DHL\ShipmentApiService"
                 id="bitbag.shopware_dhl_app.api.shipment_sender" public="true">
            <argument type="service" id="bitbag.shopware_dhl_app.api.api_resolver"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.package_factory"/>
        </service>

        <service class="BitBag\ShopwareDHLApp\API\Shopware\CustomFieldFilter"
                 id="bitbag.shopware_dhl_app.factory.custom_field_filter">
            <argument type="service" id="bitbag.shopware_dhl_app.provider.custom_field_names"/>
            <argument type="service" id="bitbag.shopware_dhl_app.api.custom_field_api_service"/>
        </service>

        <service class="BitBag\ShopwareDHLApp\API\DHL\LabelApiService"
                 id="bitbag.shopware_dhl_app.api.label_service">
            <argument type="service" id="bitbag.shopware_dhl_app.api.api_resolver"/>
        </service>

        <service class="BitBag\ShopwareDHLApp\API\Shopware\CustomFieldApiService"
                 id="bitbag.shopware_dhl_app.api.custom_field_api_service">
            <argument type="service" id="shopware.repository.custom_field"/>
            <argument type="service" id="shopware.repository.custom_field_set"/>
        </service>

        <service class="BitBag\ShopwareDHLApp\API\DHL\ShippingMethodApiService"
                 id="bitbag.shopware_dhl_app.api.shipping_method_api_service">
            <argument type="service" id="shopware.repository.shipping_method"/>
            <argument type="service" id="shopware.repository.delivery_time"/>
            <argument type="service" id="shopware.repository.rule"/>
        </service>
    </services>
</container>
