<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service class="BitBag\ShopwareAppSkeleton\API\CustomFieldSetCreator"
                 id="bitbag.shopware_dhl_app.api.create_custom_field_set">
            <argument type="service" id="bitbag.shopware_dhl_app.api.client_api"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.create_custom_field_set"/>
        </service>

        <service class="BitBag\ShopwareAppSkeleton\API\CustomFieldsCreator"
                 id="bitbag.shopware_dhl_app.api.create_custom_fields">
            <argument type="service" id="bitbag.shopware_dhl_app.factory.create_details_package"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.create_custom_field"/>
            <argument type="service" id="bitbag.shopware_dhl_app.api.create_custom_field_set"/>
        </service>

        <service class="BitBag\ShopwareAppSkeleton\API\ClientApiService"
                 id="bitbag.shopware_dhl_app.api.client_api">
        </service>

        <service class="BitBag\ShopwareAppSkeleton\API\ApiService"
                 id="bitbag.shopware_dhl_app.api.api_service">
            <argument type="service" id="bitbag.shopware_dhl_app.repository.config_repository"/>
        </service>

        <service class="BitBag\ShopwareAppSkeleton\API\AvailabilityRuleCreator"
                 id="bitbag.shopware_dhl_app.api.availability_rule_creator">
        </service>

        <service class="BitBag\ShopwareAppSkeleton\API\ShipmentSender" id="bitbag.shopware_dhl_app.api.shipment_sender" public="true">
            <argument type="service" id="bitbag.shopware_dhl_app.repository.config_repository"/>
            <argument type="service" id="bitbag.shopware_dhl_app.api.api_service"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.sender_address"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.receiver_address"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.package_details"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.shipment"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.payment"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.service_definition"/>
        </service>
    </services>
</container>
