<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service class="BitBag\ShopwareDHLApp\EventSubscriber\AppActivatedEventSubscriber" id="bitbag.shopware_dhl_app.event_subscriber.app_activated_subscriber">
            <argument type="service" id="bitbag.shopware_dhl_app.api.create_custom_fields"/>
            <argument type="service" id="bitbag.shopware_dhl_app.api.shipping_method"/>
            <argument type="service" id="bitbag.shopware_dhl_app.factory.create_shipping_method"/>
            <argument type="service" id="bitbag.shopware_dhl_app.api.availability_rule_creator"/>
            <argument type="service" id="shopware.repository.shipping_method"/>
            <tag name="kernel.event_subscriber"/>
        </service>
    </services>
</container>
