<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service class="BitBag\ShopwareAppSkeleton\API\CreateShipment" id="bitbag.shopware_dhl_app.api.create_shipment" public="true">
            <argument type="service" id="bitbag.shopware_dhl_app.repository.config_repository"/>
        </service>

        <service class="BitBag\ShopwareAppSkeleton\Controller\OrderController" id="bitbag.shopware_dhl_app.controller.order" public="true">
            <argument type="service" id="bitbag.shopware_dhl_app.api.create_shipment"/>
        </service>

        <service class="BitBag\ShopwareAppSkeleton\Controller\APISettingsController" id="bitbag.shopware_dhl_app.controller.api_settings" public="true">
            <argument type="service" id="twig"/>
            <argument type="service" id="form.factory"/>
            <argument type="service" id="bitbag.shopware_dhl_app.repository.config_repository"/>
            <argument type="service" id="bitbag.shopware_app_skeleton.repository.shop_repository"/>
            <argument type="service" id="doctrine.orm.default_entity_manager"/>
            <argument type="service" id="translator"/>
        </service>
    </services>
</container>
